    <LinearLayout android:orientation="horizontal"
        android:background="#A6504F4F"
        android:id="@+id/funcbtns"
        android:layout_width="fill_parent"
        android:paddingBottom="3dp"
        android:paddingTop="1dp"
        android:layout_height="40dp"
        android:layout_marginTop="80dip"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true">
        <Button
            android:id="@+id/btn_restart"
            android:layout_width="75dip"
            android:layout_height="35dip"
            android:layout_marginLeft="5dip"
            android:layout_marginRight="1dip"
            android:background="@drawable/img_btn_restart"/>
        <Button
            android:id="@+id/btn_undo"
            android:layout_width="75dip"
            android:layout_height="30dp"
            android:layout_marginLeft="2dip"
            android:layout_marginRight="1dip"
            android:background="@drawable/img_btn_undo" />
        <Button
            android:id="@+id/btn_redo"
            android:layout_width="75dip"
            android:layout_height="35dip"
            android:layout_marginLeft="2dip"
            android:layout_marginRight="1dip"
            android:background="@drawable/img_btn_redo" />
        <Button
            android:id="@+id/btn_hint"
            android:layout_width="75dip"
            android:layout_height="35dip"
            android:layout_marginLeft="2dip"
            android:layout_marginRight="0dip"
            android:background="@drawable/img_btn_hints_on" />
    </LinearLayout>
    <LinearLayout
        android:background="#A6504F4F"
        android:orientation="horizontal"
        android:id="@+id/chesscnt"
        android:paddingTop="16dip"
        android:layout_width="fill_parent"
        android:layout_height="55dp"
        android:layout_marginBottom="0dip"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        >
        <ImageView
            android:id="@+id/img_blackchess"
            android:layout_width="24dip"
            android:layout_height="24dip"
            android:layout_marginBottom="2dip"
            android:layout_marginLeft="15dp"
            android:src="@drawable/black_chess" />
        <TextView
            android:textSize="24dip"
            android:textColor="#ffFFFFFF"
            android:id="@+id/txt_cnt_black"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="-4dip"
            android:layout_marginRight="0dip"
            android:text="   2 " />
        <ImageView
            android:id="@+id/img_whitechess"
            android:layout_width="24dip"
            android:layout_height="24dip"
            android:layout_marginBottom="2dip"
            android:src="@drawable/white_chess" />
